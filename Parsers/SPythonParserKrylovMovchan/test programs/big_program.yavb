def fact(n: int) -> int:
    if n == 0:
        return 1
    elif n == 1:
      return 1
    else:
     return n * fact(n - 1)


def f(a: int) -> int:
    if a == 0:
        return a
    else:
        return g(a // 2) + a

def g(b: int) -> int:
    if b == 0:
        return b
    else:
      return b + f(b - 1)
        


